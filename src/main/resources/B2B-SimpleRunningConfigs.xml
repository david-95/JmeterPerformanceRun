<?xml version="1.0" encoding="UTF-8"?>
<suites name="B2B-Run">
	<!-- threads 100 , B2B-LOGIN-MAIN-LOGOUT -->
	<suite name="B2B_Login_mainPage_Logout_100t_1r_100">
		<!--templatePath -->
		<templatePath>c:/tmp/B2B-Login_mainPage_Logout.jmx
		</templatePath>
		
		<jmxFolder>c:/tmp</jmxFolder>
		
		<!-- output of stastics files -->
		<outputFolder>/root/scripts/b2b_guanglan/outputs</outputFolder>

		<JmxPropertiesSetting>
			<!-- Customized setting of thread group -->
			<ThreadGroup.num_threads>100</ThreadGroup.num_threads>
			<ThreadGroup.ramp_time>1</ThreadGroup.ramp_time>

			<!-- for main loop controller -->
			<LoopController.continue_forever>false
			</LoopController.continue_forever>
			<LoopController.loops>-2</LoopController.loops>

			<!-- for scheduler -->
			<ThreadGroup.scheduler>true</ThreadGroup.scheduler>
			<ThreadGroup.duration>100</ThreadGroup.duration>
			
			<CSVDataSet.filepath>/root/scripts/b2b_guanglan/b2b_users.csv</CSVDataSet.filepath>
		</JmxPropertiesSetting>

	</suite>

	<suite name="B2B_Login_mainPage_Logout_80t_2r_200">
		<!--template -->
		<templatePath>c:/tmp/B2B-Login_mainPage_Logout.jmx
		</templatePath>
		<jmxFolder>c:/tmp</jmxFolder>
		<!-- output of stastics files -->
		<outputFolder>/root/scripts/b2b_guanglan/outputs</outputFolder>

		<JmxPropertiesSetting>
			<!-- Customized setting of thread group -->
			<ThreadGroup.num_threads>80</ThreadGroup.num_threads>
			<ThreadGroup.ramp_time>1</ThreadGroup.ramp_time>

			<!-- for main loop controller -->
			<LoopController.continue_forever>false
			</LoopController.continue_forever>
			<LoopController.loops>-2</LoopController.loops>

			<!-- for scheduler -->
			<ThreadGroup.scheduler>true</ThreadGroup.scheduler>
			<ThreadGroup.duration>200</ThreadGroup.duration>
			
			<!-- csv data set config path -->
			<CSVDataSet.filepath>/root/scripts/b2b_guanglan/b2b_users.csv</CSVDataSet.filepath>
		</JmxPropertiesSetting>

	</suite>
</suites>